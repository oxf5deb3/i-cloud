<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>缅甸车辆管理系统-千蓝软件</title>

    <link href="/Content/MainFrame/login.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript">
        function ckLogin() {
           var href = top.window.location.href;
           if (href.indexOf('Login/Index') == -1) {
               top.window.location.href = window.location.href;
           }
        }

        ckLogin();
    </script>
</head>
<body>

    <div class="login">
        <div class="box png">
            <div class="logo png"></div>
            <div class="input">
                <div class="log">
                    <div class="name">
                        <label>用户名</label><input type="text" class="text" id="value_1" placeholder="用户名" name="value_1" tabindex="1" />
                    </div>
                    <div class="pwd">
                        <label>密　码</label><input type="password" class="text" id="value_2" placeholder="密码" name="value_2" tabindex="2" />
                        <input type="button" class="submit" tabindex="3" value="登录" />
                        <div class="check"></div>
                    </div>
                    <div class="tip"></div>
                </div>
            </div>
        </div>
        <div class="air-balloon ab-1 png"></div>
        <div class="air-balloon ab-2 png"></div>
        <div class="footer"></div>
    </div>

    <script type="text/javascript" src="/Scripts/jquery-1.12.4.js"></script>
    <script type="text/javascript" src="/Scripts/fun.base.js"></script>
    <script type="text/javascript" src="/Scripts/script.js"></script>
</body>
</html>